    <input id="credit_card_card_number" maxlength="16" name="credit_card[card_number]" size="18" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_verification_value">CVV Code</label>
    <input id="credit_card_verification_value" maxlength="4" name="credit_card[verification_value]" size="4" type="text" />
    <span style="font-size:10px;"><a href="#" onclick="$(&#x27;cvv_help&#x27;).blindDown();; return false;">What is this?</a><br /></span>
