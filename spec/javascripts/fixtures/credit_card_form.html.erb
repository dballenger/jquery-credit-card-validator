<option value="5">5 - May</option>
<option value="6">6 - June</option>
<option value="7">7 - July</option>
<option value="8">8 - August</option>
<option value="9">9 - September</option>
<option value="10">10 - October</option>
<option value="11">11 - November</option>
<option value="12">12 - December</option>
</select>
<select id="credit_card_expiration_date_1i" name="credit_card[expiration_date(1i)]">
<option selected="selected" value="2013">2013</option>
<option value="2014">2014</option>
<option value="2015">2015</option>
<option value="2016">2016</option>
<option value="2017">2017</option>
<option value="2018">2018</option>
</select>

  </div>

  
</fieldset>

<fieldset id="billing_information">
  <legend>Billing Information</legend>
  <div class="notes">
    <h4>Billing Information</h4>
    <p>
      Please enter the billing address information associated this card.  This is the address where you receive your monthly statement.
    </p>
  </div>
  <div class="required">
    <label for="credit_card_first_name">First name</label>
    <input id="credit_card_first_name" name="credit_card[first_name]" size="20" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_last_name">Last name</label>
    <input id="credit_card_last_name" name="credit_card[last_name]" size="20" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_street_address">Street address</label>
    <input id="credit_card_street_address" name="credit_card[street_address]" size="30" type="text" />
  </div>
  <div>
    <label for="credit_card_street_address_2">Street address 2</label>
    <input id="credit_card_street_address_2" name="credit_card[street_address_2]" size="30" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_city">City</label>
    <input id="credit_card_city" name="credit_card[city]" size="20" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_state">State</label>
    <input id="credit_card_state" name="credit_card[state]" size="20" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_zipcode">Zipcode</label>
    <input id="credit_card_zipcode" maxlength="10" name="credit_card[zipcode]" size="10" type="text" />
  </div>
  <div class="required">
    <label for="credit_card_country">Country</label>
    <select id="credit_card_country" name="credit_card[country]"><option value=""></option>
<option value="United States">United States</option>
<option value="Canada">Canada</option>
<option value="France">France</option></select>
  </div>
  <div class="required">
    <label for="credit_card_phone_number">Phone number</label>
    <input id="credit_card_phone_number" name="credit_card[phone_number]" size="15" type="text" />
  </div>
</fieldset>

  <label for="submit_tag">&nbsp;</label><input data-disable-with="Processing Request..." id="submit_tag" name="commit" type="submit" value="Add Card" />
</form>